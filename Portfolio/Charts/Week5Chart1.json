{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

    "data": {
       "url": "https://raw.githubusercontent.com/miasutcliffe/miasutcliffe.github.io/main/data_ONS-KG9W.json",
       "format": {
           "type":"json",
           "property": "years"}},


    "title": {
        "text": "Import Goods Annual Growth Rate CVM SA% ",
        "subtitle": "Source: ONS" ,
        "subtitleFontStyle":"italic",
        "subtitleFontSize":10,
        "anchor": "start",
        "color": "black"},

    "width":300,
    "height":300,

    "mark":{
        "type": "line",
        "color":"seagreen",
        "strokeWidth":2,
        "opacity":1},
 

    "encoding": {
      "x": {
          "field": "date",
          "type": "temporal",
          "title":"Year"},
      "y": {
          "field": "value",
          "type": "quantitative",
          "title":"%"}
    }}